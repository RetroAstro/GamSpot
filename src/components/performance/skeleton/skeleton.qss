.skeleton-box {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #fff;
  opacity: 1;
}

.fadeOut {
  opacity: 0;
  transition: all 0.5s ease-in-out;
}

.shine {
  width: 100%;
  height: 100%;
  background: rgb(245, 245, 245);
  animation-duration: 2s;
  animation-name: blink;
  animation-iteration-count: infinite;
}
